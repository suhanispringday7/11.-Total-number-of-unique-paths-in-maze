#include <stdio.h>
int n, m, maze[10][10];
int countPaths(int i, int j) {
if (i == n - 1 && j == m - 1) return 1;
if (i >= n || j >= m || maze[i][j] == 0) return 0;
return countPaths(i + 1, j) + countPaths(i, j + 1);
}
int main() {
scanf("%d %d", &n, &m);
for (int i = 0; i < n; i++)
for (int j = 0; j < m; j++)
scanf("%d", &maze[i][j]);
printf("%d\n", countPaths(0, 0));
return 0;
}
